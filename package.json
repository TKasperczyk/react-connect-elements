{
  "name": "react-connect-elements",
  "version": "0.1.1",
  "description": "Connect elements with SVG",
  "license": "MIT",
  "author": "Emerson Laurentino <emersonlaurentino@hotmail.com>",
  "homepage": "https://github.com/emersonlaurentino/react-connect-elements",
  "main": "dist/react-connect-elements.js",
  "source": "src/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "demo:build": "parcel build --out-dir demo/dist demo/index.html --public-url=/react-connect-elements",
    "demo:deploy": "yarn demo:build && gh-pages -d demo/dist",
    "demo:dev": "parcel --out-dir demo/dist demo/index.html",
    "lint": "esw -c .eslintrc src --ext .js --ignore-path .eslintignore --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emersonlaurentino/react-connect-elements"
  },
  "bugs": {
    "url": "https://github.com/emersonlaurentino/react-connect-elements/issues"
  },
  "keywords": [
    "react",
    "react-connect-elements",
    "component",
    "svg",
    "connect"
  ],
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "gh-pages": "^1.1.0",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
